{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/build_Linux_x86_64/MARTe/MARTe.ex",
            "args": ["${workspaceFolder}/build/build_Linux_x86_64/Examples/WaterTank/MARTe-WaterTank.cfg"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/build_Linux_x86_64/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}